{
  "name": "icubed-react-js",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:mock": "cross-env VITE_ENABLE_MSW=true vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:cicd": "vite preview --port 4173 --host 0.0.0.0",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:cypress": "cypress open",
    "cy:run": "cypress run",
    "cy:pipelineRun:chrome": "cypress run --browser chrome --config baseUrl=http://127.0.0.1:4173",
    "cy:pipelineRun:edge": "cypress run --browser edge --config baseUrl=http://127.0.0.1:4173",
    "cy:pipelineRun:firefox": "cypress run --browser firefox --config baseUrl=http://127.0.0.1:4173",
    "test:e2e": "start-server-and-test 'npm run preview' http://127.0.0.1:4173/index.html 'npm run cy:run'",
    "test:e2e:chrome": "start-server-and-test 'npm run preview:cicd' http://127.0.0.1:4173/index.html 'npm run cy:pipelineRun:chrome'",
    "test:e2e:edge": "start-server-and-test 'npm run preview:cicd' http://127.0.0.1:4173/index.html 'npm run cy:pipelineRun:edge'",
    "test:e2e:firefox": "start-server-and-test 'npm run preview:cicd' http://127.0.0.1:4173/index.html 'npm run cy:pipelineRun:firefox'",
    "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@tailwindcss/vite": "^4.1.15",
    "graphql": "^16.11.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-router": "^7.9.4",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/coverage-v8": "^4.0.1",
    "@vitest/ui": "^4.0.1",
    "cross-env": "^10.1.0",
    "cypress": "^15.5.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "msw": "^2.11.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "start-server-and-test": "^2.1.2",
    "vite": "^7.1.7",
    "vitest": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
